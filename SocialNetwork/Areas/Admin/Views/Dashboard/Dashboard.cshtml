<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Admiro admin is super flexible, powerful, clean &amp; modern responsive bootstrap 5 admin template with unlimited possibilities.">
    <meta name="keywords" content="admin template, Admiro admin template, best javascript admin, dashboard template, bootstrap admin template, responsive admin template, web app">
    <meta name="author" content="pixelstrap">
    <title>@ViewData["Title"] - Admin Panel</title>
    <!-- Favicon icon-->
    <link rel="icon" href="~/assets/images/favicon.png" type="image/x-icon" asp-append-version="true">
    <link rel="shortcut icon" href="~/assets/images/favicon.png" type="image/x-icon" asp-append-version="true">
    <!-- Google font-->
    <link rel="preconnect" href="https://fonts.googleapis.com" asp-append-version="true">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="" asp-append-version="true">
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:opsz,wght@6..12,200;6..12,300;6..12,400;6..12,500;6..12,600;6..12,700;6..12,800;6..12,900;6..12,1000&amp;display=swap" rel="stylesheet" asp-append-version="true">
    <!-- Flag icon css -->
    <link rel="stylesheet" href="~/assets/css/vendors/flag-icon.css" asp-append-version="true">
    <!-- iconly-icon-->
    <link rel="stylesheet" href="~/assets/css/iconly-icon.css" asp-append-version="true">
    <link rel="stylesheet" href="~/assets/css/bulk-style.css" asp-append-version="true">
    <!-- iconly-icon-->
    <link rel="stylesheet" href="~/assets/css/themify.css" asp-append-version="true">
    <!--fontawesome-->
    <link rel="stylesheet" href="~/assets/css/fontawesome-min.css" asp-append-version="true">
    <!-- Whether Icon css-->
    <link rel="stylesheet" type="text/css" href="~/assets/css/vendors/weather-icons/weather-icons.min.css" asp-append-version="true">
    <link rel="stylesheet" type="text/css" href="~/assets/css/vendors/dropzone.css" asp-append-version="true">
    <link rel="stylesheet" type="text/css" href="~/assets/css/vendors/scrollbar.css" asp-append-version="true">
    <link rel="stylesheet" type="text/css" href="~/assets/css/vendors/slick.css" asp-append-version="true">
    <link rel="stylesheet" type="text/css" href="~/assets/css/vendors/slick-theme.css" asp-append-version="true">
    <!-- App css -->
    <link id="color" rel="stylesheet" href="~/assets/css/color-1.css" media="screen" asp-append-version="true">
    <link rel="stylesheet" href="~/assets/css/style.css" asp-append-version="true">
    <link href="~/assets/css/responsive.css" rel="stylesheet" type="text/css" />
    <!-- DataTables CSS -->
    <link href="~/assets/css/datatables.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/datatable-extension.css" rel="stylesheet" type="text/css" />
    <style>
        /* Hide loader */
        .loader-wrapper {
            display: none !important;
        }
    </style>
</head>
<body>
    <!-- page-wrapper Start-->
    <!-- tap on top starts-->
    <div class="tap-top"><i class="iconly-Arrow-Up icli"></i></div>
    <!-- tap on tap ends-->
    <!-- loader-->
    <!-- <div class="loader-wrapper">
        <div class="loader"><span></span><span></span><span></span><span></span><span></span></div>
    </div> -->
    <div class="page-wrapper" id="pageWrapper">
        <!-- Page Header Start-->
        <header class="page-header">
            <div class="header-wrapper">
                <div class="header-logo-wrapper">
                    <div class="logo-wrapper">
                        <a href="/Admin/Dashboard"><img class="img-fluid" src="~/assets/images/logo/logo.png" alt=""></a>
                    </div>
                    <div class="toggle-sidebar">
                        <i class="status_toggle middle sidebar-toggle" data-feather="align-center"></i>
                    </div>
                </div>
                <div class="nav-right col-12 pull-right right-header p-0">
                    <ul class="nav-menus">
                        <li class="profile-nav custom-dropdown ms-2">
                            <div class="user-wrap d-flex align-items-center">
                                <div class="user-img me-3">
                                    <img src="~/assets/images/profile.png" alt="user" class="rounded-circle" style="width: 40px; height: 40px; object-fit: cover;">
                                </div>
                                <div class="user-content me-3">
                                    <h6 class="text-end mb-1">Admin</h6>
                                    <p class="mb-0 text-end">Administrator<i class="fa-solid fa-chevron-down ms-2"></i></p>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </header>
        <!-- Page Header Ends -->

        <!-- Page Body Start-->
        <div class="page-body-wrapper">
            <!-- Page sidebar start-->
            <aside class="page-sidebar">
                <div class="left-arrow" id="left-arrow"><i class="feather-arrow"></i></div>
                <div class="main-sidebar" id="main-sidebar">
                    <ul class="sidebar-menu" id="simple-bar">
                        <li class="sidebar-main-title">
                            <div>
                                <h5 class="f-w-700 sidebar-title">Dashboards</h5>
                            </div>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/AppUsers">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Profile"></use>
                                </svg>
                                <h6>AppUsers</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/Chats">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Chat"></use>
                                </svg>
                                <h6>Chats</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/CourseSubscriptions">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Paper"></use>
                                </svg>
                                <h6>Course Subscriptions</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/Courses">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Document"></use>
                                </svg>
                                <h6>Courses</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/Friendships">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#User"></use>
                                </svg>
                                <h6>Friendships</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/Groups">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Work"></use>
                                </svg>
                                <h6>Groups</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/GroupSubscriptions">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Paper-Plus"></use>
                                </svg>
                                <h6>Group Subscriptions</h6>
                            </a>
                        </li>
                        <li class="sidebar-list">
                            <a class="sidebar-link" href="/Admin/Messages">
                                <svg class="stroke-icon">
                                    <use href="~/assets/svg/iconly-sprite.svg#Message"></use>
                                </svg>
                                <h6>Messages</h6>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="right-arrow" id="right-arrow"><i data-feather="arrow-right"></i></div>
            </aside>
            <!-- Page sidebar end-->

            @RenderBody()
        </div>
    </div>
    <!-- jquery-->
    <script src="~/assets/js/vendors/jquery/jquery.min.js"></script>
    <!-- bootstrap js-->
    <script src="~/assets/js/vendors/bootstrap/dist/js/bootstrap.bundle.min.js" defer=""></script>
    <script src="~/assets/js/vendors/bootstrap/dist/js/popper.min.js" defer=""></script>
    <!--fontawesome-->
    <script src="~/assets/js/vendors/font-awesome/fontawesome-min.js"></script>
    <!-- sidebar -->
    <script src="~/assets/js/sidebar.js"></script>
    <!-- dropzone-->
    <script src="~/assets/js/dropzone/dropzone.js"></script>
    <script src="~/assets/js/dropzone/dropzone-script.js"></script>
    <!-- scrollbar-->
    <script src="~/assets/js/scrollbar/simplebar.js"></script>
    <script src="~/assets/js/scrollbar/custom.js"></script>
    <!-- slick-->
    <script src="~/assets/js/slick/slick.min.js"></script>
    <script src="~/assets/js/slick/slick.js"></script>
    <!-- theme_customizer-->
    <script src="~/assets/js/theme-customizer/customizer.js"></script>
    <!-- DataTables -->
    <script src="~/assets/js/datatable/datatables/jquery.dataTables.min.js"></script>
    <script src="~/assets/js/datatable/datatable-extension/dataTables.buttons.min.js"></script>
    <script src="~/assets/js/datatable/datatable-extension/jszip.min.js"></script>
    <script src="~/assets/js/datatable/datatable-extension/buttons.html5.min.js"></script>
    <script src="~/assets/js/datatable/datatable-extension/buttons.print.min.js"></script>
    <!-- custom script -->
    <script src="~/assets/js/script.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>